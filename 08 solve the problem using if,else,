#include<stdio.h>
int main()
{
   int age;
   char id;
   
   printf("Enter your age\n");
   scanf("%d",&age);

   if (age<18)
   {
       printf("your not elegible for voting try next time \n");
   }

   else if(age==18){
       printf("do you have voter id enter y,not have enter n\n");
       scanf("%s",&id);
       if (id=='y')
       {
           printf("your elegible for voting\n");

       }
       else if(id=='n'){
           printf("your not elegible for voting ,apply voter id\n");
       }
       else{
           printf("your option is worng");
       }
   }  
   else if(age>18){
       
       printf("do you have voter id enter y ,not have enter n\n");
       scanf("%s",&id);
       if (id=='y')
       {
           printf("your elegible for voting\n");

       }
       else if(id=='n'){
           printf("your not elegible for voting ,bring your id card \n");
       }
       else{
           printf("your option is worng");
       }
    }
   return 0;
}